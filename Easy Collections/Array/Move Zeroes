class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        count = 0
        k = 0
        while count < len(nums):
            if nums[k] == 0:
                nums.append(0)
                del nums[k]
                count += 1
            else:
                k += 1
                count += 1
