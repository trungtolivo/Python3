class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        k = -1
        digits[-1] += 1
        while k > -(len(digits)):
            if digits[k] == 10:
                digits[k-1] += 1
                digits[k] = 0
                k -= 1
            else:
                break
        if digits[0] == 10:
            digits.append(0)
            digits[0] = 1
        return digits
