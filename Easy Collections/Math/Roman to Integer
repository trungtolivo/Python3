class Solution:
    def romanToInt(self, s: str) -> int:
        roman = {'M':1000, 'D':500, 'C':100, 'L':50, 'X':10, 'V':5, 'I':1}
        sums = 0
        for i in range(1,len(s)):
            if roman[s[i]] <= roman[s[i-1]]:
                sums += roman[s[i-1]]
            elif roman[s[i]] > roman[s[i-1]]:
                sums -= roman[s[i-1]]
        return sums + roman[s[-1]]
